{
    "container": {
        "type": "DOCKER",
        "docker": {
            "image": "joshand/chronic_ucs_esx_analyzer:latest",
            "forcePullImage": true,
            "network": "BRIDGE",
            "portMappings": [{
                "containerPort": 5000,
                "hostPort": 0
            }]
        },
        "forcePullImage": true
    },
    "healthChecks": [{
        "protocol": "TCP",
        "portIndex": 0
    }],
    "id": "/imapex/chronic/ucs-esx-analyzer",
    "instances": 1,
    "cpus": 0.1,
    "mem": 16,
    "env": {
        "CHRONICBUS": "http://imapex-chronic-bus.app.mantldevnetsandbox.com"
    }
}
